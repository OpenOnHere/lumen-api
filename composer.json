{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "lucadegasperi/oauth2-server-laravel": "^5.0",
        "dingo/api": "1.0.x@dev",
        "tymon/jwt-auth": "^0.5.5",
        "basicit/lumen-vendor-publish": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0",
        "zircote/swagger-php": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/tralves/Swaggervel"
        }
    ]
}
